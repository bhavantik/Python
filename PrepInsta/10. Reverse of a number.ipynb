   ],
   "source": [
    "rev=0\n",
    "reminder=0\n",
    "\n",
    "while num > 0:\n",
    "    reminder=num%10\n",
    "    num= num//10 \n",
    "    rev = rev*10 + reminder\n",
    "print(rev)"
   ]
